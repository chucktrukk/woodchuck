{
  "name": "Precision Nutrition / Gmail Integration",
  "description": "Integrates Precision Nutrition details into Gmail",
  "options_page": "options/options.html",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "assets/images/logo16.png",
    "48": "assets/images/logo48.png",
    "128": "assets/images/logo128.png"
  },
  "browser_action": {
    "default_title": "Precision Nutrition",
    "default_icon": "assets/images/logo19.png"
  },
  "web_accessible_resources": [
    "vendor/jquery.js",
    "vendor/gmail.js",
    "vendor/sugar.min.js",
    "lib/camelize.js",
    "squawker.js",
    "assets/css/style.css",
    "assets/images/logo48.png"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": [
        "vendor/jquery.js",
        "vendor/yepnope.min.js",
        "woodchuck.js",
        "woodchuck/authentication.js",
        "woodchuck/listener.js",
        "woodchuck/viewport.js",
        "woodchuck/injector.js"
      ]
    }
  ]
}
